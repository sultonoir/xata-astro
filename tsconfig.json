{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@lib/*": ["src/lib/*"],
      "@/*": ["./src/*"],
      "@public/*": ["./public/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
